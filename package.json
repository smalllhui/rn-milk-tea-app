{
  "name": "tea-mall-app",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "expo": "~51.0.24",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.8.0",
    "@types/react": "~18.2.45",
    "commitizen": "^4.3.0",
    "commitlint": "^19.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.2.1",
    "eslint": "9.x",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.0.0"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  }
}
